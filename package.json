{
  "name": "keyword-search-database",
  "displayName": "Keyword Search Database",
  "description": "Extension giúp bạn tìm kiếm một từ khóa trong database",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "keyword-search-database.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "keyword-search-database.tabConnectionManager",
        "title": "Open connection mamager tab"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+c",
        "command": "keyword-search-database.tabConnectionManager",
        "mac": "cmd+alt+c"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "package": "NODE_ENV=production node ./esbuild.js",
    "watch": "node ./esbuild.js --watch",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@types/vscode-webview": "^1.57.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "esbuild": "^0.16.10",
    "esbuild-plugin-copy": "^2.0.1",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "monaco-editor": "^0.36.1"
  }
}
